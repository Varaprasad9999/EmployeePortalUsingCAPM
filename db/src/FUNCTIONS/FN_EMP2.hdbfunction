FUNCTION "FN_EMP2"(INPUT_ID INTEGER, INPUT_NAME NVARCHAR(100) )
    RETURNS TABLE( EMPID INTEGER, FIRSTNAME NVARCHAR(100))
    LANGUAGE SQLSCRIPT
    SQL SECURITY INVOKER AS
BEGIN
    /*****************************
        Write your function logic
    ****************************/

RETURN

    SELECT EMPID, FIRSTNAME FROM "DATA_EMPLOYEE" 
    WHERE EMPID = INPUT_ID AND FIRSTNAME = INPUT_NAME ;
END;