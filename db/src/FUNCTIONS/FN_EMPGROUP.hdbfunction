FUNCTION "FN_EMPGROUP"( )
    RETURNS TABLE(DEPARTMENTNAME NVARCHAR(100), EMPLOYEE_COUNT INTEGER )
    LANGUAGE SQLSCRIPT
    SQL SECURITY INVOKER AS
BEGIN
    /*****************************
        Write your function logic
    ****************************/
RETURN

SELECT D.DEPARTMENTNAME, COUNT(E.EMPID) AS EMPLOYEE_COUNT
FROM "DATA_EMPLOYEE" AS E
INNER JOIN "DATA_DEPARTMENT" AS D ON E.DEPARTMENTID = D.DEPARTMENTID
GROUP BY D.DEPARTMENTNAME
;


END;